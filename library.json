{
    "name": "OpenIndus",
    "version": "1.5.0",
    "keywords": "OpenIndus",
    "description": "OpenIndus Library",
    "repository": {
        "type": "git",
        "url": "https://github.com/openindus"
    },
    "authors": [
        {
            "name": "OpenIndus",
            "email": "contact@openindus.com",
            "url": "https://openindus.com"
        }
    ],
    "frameworks": ["espidf", "arduino"],
    "platforms": "espressif32",
    "export": {
        "include": [
            "boards",
            "components",
            "examples",
            "README.md"
        ],
        "exclude": [
            "components/libraries/Cloud",
            "components/libraries/Modem",
            "components/libraries/Netif",
            "components/libraries/Ethernet",
            "components/libraries/USBHost"
        ]
    },
    "build": {
        "flags": [
            "-I components/api",
            "-I components/api/System",
            "-I components/api/Controller",
            "-I components/api/Controller/Master",
            "-I components/api/Controller/Slave",
            "-I components/api/Cmds",
            "-I components/api/Cmds/Analog",
            "-I components/api/Cmds/Digital",
            "-I components/api/Cmds/Relay",
            "-I components/api/Cmds/Stepper",
            "-I components/api/Cmds/Dc",
            "-I components/api/Cmds/Encoder",
            "-I components/api/Module",
            "-I components/api/Module/AnalogLS",
            "-I components/api/Module/Brushless",
            "-I components/api/Module/Core",
            "-I components/api/Module/Discrete",
            "-I components/api/Module/Mixed",
            "-I components/api/Module/RelayHP",
            "-I components/api/Module/Stepper",
            "-I components/api/Module/Dc",
            "-I components/middleware/esp32",
            "-I components/middleware/esp32/Analog",
            "-I components/middleware/esp32/Analog/InputsHV",
            "-I components/middleware/esp32/Analog/InputsLS",
            "-I components/middleware/esp32/Analog/InputsLS/ADC",
            "-I components/middleware/esp32/Analog/InputsLS/ADC/ADS114S0X",
            "-I components/middleware/esp32/Analog/InputsLS/Multiplexer",
            "-I components/middleware/esp32/Analog/InputsLS/Digipot",
            "-I components/middleware/esp32/Analog/InputsLS/Sensors",
            "-I components/middleware/esp32/Analog/InputsLS/Sensors/RawSensor",
            "-I components/middleware/esp32/Analog/InputsLS/Sensors/RTD",
            "-I components/middleware/esp32/Analog/InputsLS/Sensors/Thermocouple",
            "-I components/middleware/esp32/Analog/InputsLS/Sensors/StrainGauge",
            "-I components/middleware/esp32/Analog/InputsLV",
            "-I components/middleware/esp32/Analog/Outputs",
            "-I components/middleware/esp32/Board",
            "-I components/middleware/esp32/Bus",
            "-I components/middleware/esp32/CAN",
            "-I components/middleware/esp32/Digital",
            "-I components/middleware/esp32/Digital/Inputs",
            "-I components/middleware/esp32/Digital/Outputs",
            "-I components/middleware/esp32/Encoder",
            "-I components/middleware/esp32/FlashLoader",
            "-I components/middleware/esp32/Led",
            "-I components/middleware/esp32/Motor",
            "-I components/middleware/esp32/Motor/Brushless",
            "-I components/middleware/esp32/Motor/Stepper",
            "-I components/middleware/esp32/Motor/Dc",
            "-I components/middleware/esp32/Relays",
            "-I components/middleware/esp32/RS",
            "-I components/middleware/esp32/RTClock",
            "-I components/middleware/esp32/Usb",
            "-I components/drivers/esp32",
            "-I components/drivers/esp32/adc",
            "-I components/drivers/esp32/adc/ads114s0x",
            "-I components/drivers/esp32/adc/ads866x",
            "-I components/drivers/esp32/cd74hc4052",
            "-I components/drivers/esp32/dac",
            "-I components/drivers/esp32/dac/ad5413",
            "-I components/drivers/esp32/dac8760",
            "-I components/drivers/esp32/m41t62",
            "-I components/drivers/esp32/m41t62/STM32DriversComponents",
            "-I components/drivers/esp32/mcp25625",
            "-I components/drivers/esp32/mcp25625/DriversComponents",
            "-I components/drivers/esp32/pcal6524",
            "-I components/drivers/esp32/powerSTEP01",
            "-I components/drivers/esp32/sc16is750",
            "-I components/drivers/esp32/stds75",
            "-I components/libraries",
            "-I components/libraries/Modbus"
        ],
        "includeDir": ".",
        "srcDir": ".",
        "srcFilter": [
            "-<*>",
            "+<components/api>",
            "+<components/middleware/esp32>",
            "+<components/drivers/esp32>",
            "+<components/libraries/Modbus>"
        ]
    }
}
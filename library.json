{
    "name": "OpenIndus",
    "version": "1.6.2-dev",
    "keywords": "OpenIndus",
    "description": "OpenIndus Library",
    "repository": {
        "type": "git",
        "url": "https://github.com/openindus"
    },
    "authors": [
        {
            "name": "OpenIndus",
            "email": "contact@openindus.com",
            "url": "https://openindus.com"
        }
    ],
    "frameworks": ["espidf", "arduino"],
    "platforms": "espressif32",
    "export": {
        "include": [
            "boards",
            "components",
            "external_components",
            "examples",
            "README.md"
        ],
        "exclude": [
            "components/middleware/_linux_arm",
            "components/middleware/Netif",
            "components/middleware/Cloud",
            "external_components/Arduino/Ethernet",
            "external_components/Arduino/USBHost"
        ]
    },
    "build": {
        "flags": [
            "-I components/api",
            "-I components/api/System",
            "-I components/api/Master",
            "-I components/api/Slave",
            "-I components/api/Controller",
            "-I components/api/Controller/AnalogCmd",
            "-I components/api/Controller/DigitalCmd",
            "-I components/api/Controller/RelayCmd",
            "-I components/api/Controller/MotorCmd",
            "-I components/api/Controller/DcCmd",
            "-I components/api/Controller/EncoderCmd",
            "-I components/api/Module",
            "-I components/api/Module/AnalogLS",
            "-I components/api/Module/Brushless",
            "-I components/api/Module/Core",
            "-I components/api/Module/Discrete",
            "-I components/api/Module/Mixed",
            "-I components/api/Module/RelayHP",
            "-I components/api/Module/Stepper",
            "-I components/api/Module/Dc",
            "-I components/middleware",
            "-I components/middleware/Analog",
            "-I components/middleware/Analog/InputsHV",
            "-I components/middleware/Analog/InputsLS",
            "-I components/middleware/Analog/InputsLS/ADC",
            "-I components/middleware/Analog/InputsLS/ADC/ADS114S0X",
            "-I components/middleware/Analog/InputsLS/Multiplexer",
            "-I components/middleware/Analog/InputsLS/Digipot",
            "-I components/middleware/Analog/InputsLS/Sensors",
            "-I components/middleware/Analog/InputsLS/Sensors/RawSensor",
            "-I components/middleware/Analog/InputsLS/Sensors/RTD",
            "-I components/middleware/Analog/InputsLS/Sensors/Thermocouple",
            "-I components/middleware/Analog/InputsLS/Sensors/StrainGauge",
            "-I components/middleware/Analog/InputsLV",
            "-I components/middleware/Analog/Outputs",
            "-I components/middleware/Board",
            "-I components/middleware/Bus",
            "-I components/middleware/CAN",
            "-I components/middleware/Digital",
            "-I components/middleware/Digital/Inputs",
            "-I components/middleware/Digital/Outputs",
            "-I components/middleware/Encoder",
            "-I components/middleware/FlashLoader",
            "-I components/middleware/Led",
            "-I components/middleware/Modem",
            "-I components/middleware/Motor",
            "-I components/middleware/Motor/Brushless",
            "-I components/middleware/Motor/Stepper",
            "-I components/middleware/Motor/Dc",
            "-I components/middleware/Relays",
            "-I components/middleware/RS",
            "-I components/middleware/RTClock",
            "-I components/middleware/Usb",
            "-I components/drivers",
            "-I components/drivers/adc",
            "-I components/drivers/adc/ads114s0x",
            "-I components/drivers/adc/ads866x",
            "-I components/drivers/cd74hc4052",
            "-I components/drivers/dac",
            "-I components/drivers/dac/ad5413",
            "-I components/drivers/dac8760",
            "-I components/drivers/m41t62",
            "-I components/drivers/m41t62/STM32DriversComponents",
            "-I components/drivers/mcp25625",
            "-I components/drivers/mcp25625/DriversComponents",
            "-I components/drivers/pcal6524",
            "-I components/drivers/powerSTEP01",
            "-I components/drivers/sc16is750",
            "-I components/drivers/stds75",
            "-I external_components/Arduino/Modbus/src"
        ],
        "includeDir": ".",
        "srcDir": ".",
        "srcFilter": [
            "-<*>",
            "+<components/api>",
            "+<components/middleware>",
            "-<components/middleware/_linux_arm>",
            "-<components/middleware/Netif>",
            "-<components/middleware/Cloud>",
            "+<components/drivers>",
            "+<external_components/Arduino/Modbus>"
        ]
    }
}